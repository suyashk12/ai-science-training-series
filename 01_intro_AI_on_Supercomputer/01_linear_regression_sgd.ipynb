        "  learning_rate_b = learning_rates_b[i]\n",
        "\n",
        "  # Adjusted loop size to account that entire dataset is considered\n",
        "  loop_N = 30*len(data)//batch_size\n",
        "  steps.append(loop_N)\n",
        "  loss_history = []\n",
        "\n",
        "  for i in range(loop_N):\n",
        "\n",
        "    # Sample with replacement\n",
        "    data_sample = data.sample(batch_size)\n",
        "    # Extract x and y\n",
        "    data_x = data_sample['GrLivArea'].to_numpy()\n",
        "    data_y = data_sample['SalePrice'].to_numpy()\n",
        "\n",
